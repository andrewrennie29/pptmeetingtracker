$("ul.errors").html("")
<% if @meeting.errors.any? %>
  <% @meeting.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".meeting-index").html("<%= escape_javascript(render 'index') %>")
  $("#meeting-modal").modal("hide")
<% end %>
